<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBindingConfiguration" maxBufferSize="1073741824"
          maxReceivedMessageSize="1073741824" />
